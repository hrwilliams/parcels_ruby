<h1>Ship your package</h1>

<form action="/parcel">
  <div class="form-group">
    <label for="length">Length of side one</label>
    <input id="length" name="length" class="form-control" type="text">
  </div>
  <div class="form-group">
    <label for="width">Width of side two</label>
    <input id="width" name="width" class="form-control" type="text">
  </div>
  <div class="form-group">
    <label for="height">Height side three</label>
    <input id="height" name="height" class="form-control" type="text">
  </div>
  <div class="form-group">
    <label for="speed">Speed: enter 1 for regular, 2 for overnight</label>
    <input id="speed" name="speed" class="form-control" type="text">
    <div class="form-group">
      <label for="weight">Weight</label>
      <input id="weight" name="weight" class="form-control" type="text">
  </div>
  <button type="submit" class="btn">submit</button>
</form>
